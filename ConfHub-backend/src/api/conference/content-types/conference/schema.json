{
  "kind": "collectionType",
  "collectionName": "conferences",
  "info": {
    "singularName": "conference",
    "pluralName": "conferences",
    "displayName": "Conference"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Conference_title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Start_date": {
      "type": "date",
      "required": true
    },
    "Conference_time": {
      "type": "time",
      "required": false
    },
    "Conference_location": {
      "type": "string",
      "required": false
    },
    "Track_title": {
      "type": "string",
      "required": false
    },
    "Track_description": {
      "type": "text",
      "required": false
    },
    "Session_title": {
      "type": "string",
      "required": false
    },
    "Speaker_names": {
      "type": "string",
      "required": false
    },
    "Submission_deadline": {
      "type": "date",
      "required": true
    },
    "Review_deadline": {
      "type": "date",
      "required": false
    },
    "requestStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ]
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "inProgress",
        "completed"
      ]
    },
    "Organizer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organizer.organizer"
    },
    "Papers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::paper.paper",
      "mappedBy": "conference"
    },
    "AssignedSubOrganizer": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "reviewFormFields": {
      "type": "json",
      "required": false
    },
    "Conference_Topics": {
      "type": "text"
    },
    "reviewer_invitations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reviewer-invitation.reviewer-invitation",
      "mappedBy": "conference"
    }
  }
}
